{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run start:watch",
    "start:watch": "nodemon ./src/index.js",
    "db:migrate": "npx prisma migrate dev",
    "preinstall": "cd ./client && npm i",
    "build": "npx prisma generate && cd client && npm run build",
    "start": "npx wait-on tcp:stream-site-db-1:5432 && npx prisma migrate deploy && node ./src/index.js"
  },
  "author": "",
  "license": "UNLICENSED",
  "description": "",
  "devDependencies": {
    "@types/env": "file:@types/env",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.14.10",
    "@types/session": "file:@types/session",
    "prisma": "^5.16.2"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "nodemon": "^3.1.4"
  }
}
